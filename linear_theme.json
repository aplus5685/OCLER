      "disabled": "#4a4a52"
    },
    "brand": {
      "primary": "#5e6ad2",
      "secondary": "#7b82e3",
      "tertiary": "#4752c4",
      "light": "#8b92f3",
      "dark": "#3e4ab8"
    },
    "ui": {
      "border": "#2b2b32",
      "borderLight": "#1f1f24",
      "borderHover": "#3f3f46",
      "borderFocus": "#5e6ad2"
    },
    "status": {
      "success": "#26b76a",
      "warning": "#f59e0b",
      "error": "#ef4444",
      "info": "#3b82f6",
      "neutral": "#6b7280"
    },
    "accent": {
      "purple": "#8b5cf6",
      "pink": "#ec4899",
      "blue": "#3b82f6",
      "teal": "#14b8a6",
      "green": "#10b981"
    }
  },
  "typography": {
    "fontFamily": {
      "sans": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif",
      "mono": "JetBrains Mono, ui-monospace, SFMono-Regular, 'SF Mono', Consolas, 'Liberation Mono', Menlo, monospace"
    },
    "fontSize": {
      "xs": "12px",
      "sm": "14px",
      "base": "15px",
      "lg": "16px",
      "xl": "18px",
      "2xl": "20px",
      "3xl": "24px",
      "4xl": "30px",
      "5xl": "36px",
      "6xl": "48px"
    },
    "fontWeight": {
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700"
    },
    "lineHeight": {
      "tight": "1.2",
      "snug": "1.375",
      "normal": "1.5",
      "relaxed": "1.625",
      "loose": "1.75"
    },
    "letterSpacing": {
      "tighter": "-0.02em",
      "tight": "-0.01em",
      "normal": "0",
      "wide": "0.01em",
      "wider": "0.02em"
    }
  },
  "spacing": {
    "0": "0px",
    "0.5": "2px",
    "1": "4px",
    "1.5": "6px",
    "2": "8px",
    "2.5": "10px",
    "3": "12px",
    "3.5": "14px",
    "4": "16px",
    "5": "20px",
    "6": "24px",
    "7": "28px",
    "8": "32px",
    "9": "36px",
    "10": "40px",
    "11": "44px",
    "12": "48px",
    "14": "56px",
    "16": "64px",
    "20": "80px",
    "24": "96px",
    "28": "112px",
    "32": "128px"
  },
  "effects": {
    "shadow": {
      "xs": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
      "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)"
    },
    "borderRadius": {
      "none": "0px",
      "sm": "4px",
      "base": "6px",
      "md": "8px",
      "lg": "12px",
      "xl": "16px",
      "2xl": "20px",
      "3xl": "24px",
      "full": "9999px"
    },
    "transition": {
      "fast": "150ms ease",
      "base": "200ms ease",
      "slow": "300ms ease",
      "slower": "400ms ease"
    },
    "blur": {
      "sm": "4px",
      "base": "8px",
      "md": "12px",
      "lg": "16px",
      "xl": "24px"
    }
  },
  "components": {
    "button": {
      "primary": {
        "background": "#5e6ad2",
        "color": "#ffffff",
        "border": "none",
        "hover": {
          "background": "#7b82e3",
          "transform": "translateY(-1px)",
          "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)"
        },
        "active": {
          "background": "#4752c4",
          "transform": "translateY(0)"
        },
        "disabled": {
          "background": "#3a3a42",
          "color": "#6b6b7c"
        }
      },
      "secondary": {
        "background": "transparent",
        "color": "#e3e3e8",
        "border": "1px solid #2b2b32",
        "hover": {
          "background": "#16161a",
          "borderColor": "#3f3f46"
        },
        "active": {
          "background": "#1a1a1f"
        }
      },
      "ghost": {
        "background": "transparent",
        "color": "#9b9ba5",
        "border": "none",
        "hover": {
          "background": "#16161a",
          "color": "#e3e3e8"
        }
      },
      "sizes": {
        "sm": {
          "padding": "6px 12px",
          "fontSize": "14px"
        },
        "md": {
          "padding": "8px 16px",
          "fontSize": "15px"
        },
        "lg": {
          "padding": "10px 20px",
          "fontSize": "16px"
        }
      }
    },
    "input": {
      "background": "#0e0e10",
      "border": "1px solid #2b2b32",
      "color": "#e3e3e8",
      "placeholder": "#6b6b7c",
      "padding": "8px 12px",
      "borderRadius": "6px",
      "focus": {
        "borderColor": "#5e6ad2",
        "outline": "0 0 0 3px rgba(94, 106, 210, 0.2)"
      },
      "error": {
        "borderColor": "#ef4444",
        "outline": "0 0 0 3px rgba(239, 68, 68, 0.2)"
      }
    },
    "card": {
      "background": "#16161a",
      "border": "1px solid #2b2b32",
      "borderRadius": "8px",
      "padding": "16px",
      "hover": {
        "borderColor": "#3f3f46",
        "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)"
      }
    },
    "modal": {
      "backdrop": "rgba(0, 0, 0, 0.5)",
      "background": "#1f1f24",
      "borderRadius": "12px",
      "padding": "24px",
      "shadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)"
    },
    "navigation": {
      "background": "#0e0e10",
      "borderBottom": "1px solid #2b2b32",
      "height": "56px",
      "item": {
        "color": "#9b9ba5",
        "padding": "8px 12px",
        "hover": {
          "color": "#e3e3e8",
          "background": "#16161a"
        },
        "active": {
          "color": "#5e6ad2",
          "background": "rgba(94, 106, 210, 0.1)"
        }
      }
    },
    "sidebar": {
      "background": "#0e0e10",
      "width": "240px",
      "borderRight": "1px solid #2b2b32",
      "item": {
        "padding": "8px 16px",
        "color": "#9b9ba5",
        "hover": {
          "background": "#16161a",
          "color": "#e3e3e8"
        },
        "active": {
          "background": "rgba(94, 106, 210, 0.1)",
          "color": "#5e6ad2",
          "borderLeft": "2px solid #5e6ad2"
        }
      }
    },
    "table": {
      "background": "#0e0e10",
      "border": "1px solid #2b2b32",
      "header": {
        "background": "#16161a",
        "borderBottom": "1px solid #2b2b32",
        "color": "#9b9ba5",
        "fontWeight": "600"
      },
      "row": {
        "borderBottom": "1px solid #1f1f24",
        "hover": {
          "background": "#16161a"
        }
      },
      "cell": {
        "padding": "12px 16px",
        "color": "#e3e3e8"
      }
    },
    "badge": {
      "background": "#16161a",
      "color": "#e3e3e8",
      "border": "1px solid #2b2b32",
      "padding": "4px 8px",
      "borderRadius": "4px",
      "fontSize": "12px",
      "variants": {
        "success": {
          "background": "rgba(38, 183, 106, 0.1)",
          "color": "#26b76a",
          "border": "1px solid rgba(38, 183, 106, 0.2)"
        },
        "warning": {
          "background": "rgba(245, 158, 11, 0.1)",
          "color": "#f59e0b",
          "border": "1px solid rgba(245, 158, 11, 0.2)"
        },
        "error": {
          "background": "rgba(239, 68, 68, 0.1)",
          "color": "#ef4444",
          "border": "1px solid rgba(239, 68, 68, 0.2)"
        }
      }
    }
  },
  "animations": {
    "fadeIn": "fadeIn 200ms ease",
    "fadeOut": "fadeOut 200ms ease",
    "slideIn": "slideIn 300ms ease",
    "slideOut": "slideOut 300ms ease",
    "scaleIn": "scaleIn 200ms ease",
    "scaleOut": "scaleOut 200ms ease"
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  }
}